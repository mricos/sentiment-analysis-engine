while read line; do
  curl -s -X POST\
  -H "Content-Type: application/json"\
  -d '{"data": "'"$line"'"}'\
  'localhost:3000/api/nlp'
done < lockdown.txt | jq '.sentiment'

curl -s -X POST\
 -H "Content-Type: application/json" \
-d '{"data": ["good", "bad", "ok"]}' 'localhost:3000/api/nlp' | jq '.sentiment'
